{
  "default": {
    "disableAppMetrics": true
  },
  "selfDiagnostics": {
    "destination": "console",
    "level": "INFO"
  },
  "sampling": {
    "percentage": 5,
    "overrides": [
      {
        "telemetryType": "dependency",
        "percentage": 5
      },
      {
        "telemetryType": "trace",
        "percentage": 5
      },
      {
        "telemetryType": "exception",
        "percentage": 100
      },
      {
        "telemetryType": "request",
        "percentage": 0,
        "attributes": [
          {
            "key": "http.url",
            "value": ".*/actuator/health/.*",
            "matchType": "regexp"
          }
        ]
      },
      {
        "telemetryType": "request",
        "percentage": 100,
        "attributes": [
          {
            "key": "http.url",
            "value": ".*",
            "matchType": "regexp"
          }
        ]
      }
    ]
  }
}
